                    <li><a class="mmTags">Tags</a></li>
                </ul>


                <div style="float:right; margin:23px 0 0 0;">
                @if (User.Identity.IsAuthenticated)
                {
                    <a class="mmAccount" href="#">Account</a>
                    <a class="mmSep" href="#">|</a>
                    <a class="mmSignOut">Sign Out</a>
                }
                else
                {   
                    <a class="mmLogin" href="/Account/Login">Login</a>
                    <a class="mmSep">|</a>
                    <a class="mmRegister" href="/Account/Register">Register</a>
                }
                </div>
            </div>
        </div>


        <div class="container hdr-cont">
            <input type="text" id="SearchBarTxt" title="Search..." placeholder="Search..." value=""  autocomplete=”off” />
        </div>
